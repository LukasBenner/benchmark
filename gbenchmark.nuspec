<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>Google.Benchmark</id>
    <version>1.8.0</version>
    <description>Google Benchmark</description>
    <authors>Lukas Benner, Google</authors>
    <tags>Native, native</tags>
  </metadata>

  <files>
    <file src="gbenchmark.props" target="native" />  

    <!-- headers -->
    <file src="include\**\*.h"  target="native\include" />

    <!-- libs -->
    <file src="build\src\Release\*.lib"  target="native\lib\x64\Release" />
    <file src="build\src\Debug\*.lib"  target="native\lib\x64\Debug" />
    
    <file src="docs\*"   target="native\docs"    />
  </files>
</package>